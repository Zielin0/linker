// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id       Int     @id @default(autoincrement())
  alias    String  @db.VarChar(15)
  original String
  public   Boolean
  views    Int
}
